{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES2018",
    "jsx":"react",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "types": ["node", "jest", "webpack-env", "webpack"]
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "./build/", "./dist"]
}
